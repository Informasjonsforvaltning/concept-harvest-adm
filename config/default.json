{
  "server": {
    "port": 8135
  },
  "mongodb": {
    "username": "admin",
    "password": "admin",
    "host": "localhost",
    "port": 27017,
    "name": "fdk-harvest-admin"
  },
  "rabbitmq": {
    "host": "localhost",
    "port": 5672,
    "exchange": "harvests",
    "listenerKey": "concept.*.NewDataSource",
    "validationKey": "conceptPublisherNewDataSource-msg",
    "publisherKey": "concept.publisher.HarvestTrigger"
  },
  "keycloak": {
    "config": {
      "realm": "fdk",
      "bearer-only": true,
      "auth-server-url": "http://localhost:8084/auth",
      "ssl-required": "external",
      "resource": "fdk-harvest-admin",
      "verify-token-audience": true
    },
    "allowed-authority": "system:root:admin"
  },
  "spec": {
    "open-api": "fdk-harvest-admin.yaml",
    "async-api": "fdk-message-broker.yaml"
  }
}
