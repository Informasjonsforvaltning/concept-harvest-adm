{
  "name": "fdk-harvest-admin",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "db:up": "docker-compose -f docker-compose-mongo.yml up -d mongodb ",
    "db:down": "docker-compose -f docker-compose-mongo.yml stop mongodb ",
    "db:migrate:apply": "NODE_ENV=production node_modules/.bin/migrate-mongo up",
    "db:migrate:rollback": "NODE_ENV=production node_modules/.bin/migrate-mongo down",
    "db:migrate:status": "NODE_ENV=production node_modules/.bin/migrate-mongo status",
    "start": "NODE_ENV=development ts-node-dev --respawn --no-notify --transpileOnly ./src/server.ts",
    "build": "rm -rf dist && tsc",
    "serve:prod": "NODE_ENV=production node ./dist/src/server.js",
    "prod": "npm-run-all db:migrate:apply db:migrate:status serve:prod",
    "lint": "tsc --noEmit && eslint ./src ./test --ext=js,ts --fix",
    "test": "NODE_ENV=test node node_modules/mocha/bin/_mocha -r ts-node/register test/data-source.api.test.ts --exit --timeout 10000"
  },
  "type": "commonjs",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.5.5",
    "app-root-path": "^2.2.1",
    "asyncapi-validator": "^2.3.2",
    "body-parser": "^1.19.0",
    "config": "^3.2.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validate": "^0.4.4",
    "js-yaml": "^3.13.1",
    "keycloak-connect": "^7.0.1",
    "lodash": "^4.17.15",
    "migrate-mongo": "^7.0.1",
    "mongoose": "^5.7.5",
    "npm-run-all": "^4.1.5",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/chai": "^4.2.3",
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/faker": "^4.1.6",
    "@types/js-yaml": "^3.12.1",
    "@types/keycloak-connect": "^4.5.1",
    "@types/lodash": "^4.14.141",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.18",
    "@types/node": "^12.7.5",
    "@types/sinon": "^7.5.0",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "chai": "^4.2.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "mocha": "^6.2.0",
    "mongodb-memory-server": "^5.2.6",
    "prettier": "^1.18.2",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "^3.6.3"
  }
}
