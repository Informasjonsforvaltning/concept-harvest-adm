{
  "name": "fdk-harvest-admin",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "db:up": "docker-compose -f docker-compose-mongo.yml up -d mongodb ",
    "db:down": "docker-compose -f docker-compose-mongo.yml stop mongodb ",
    "db:migrate:apply": "NODE_ENV=production node_modules/.bin/migrate-mongo up",
    "db:migrate:rollback": "NODE_ENV=production node_modules/.bin/migrate-mongo down",
    "db:migrate:status": "NODE_ENV=production node_modules/.bin/migrate-mongo status",
    "start": "NODE_ENV=development ts-node-dev ./src/server.ts",
    "build": "rm -rf dist && tsc",
    "serve:prod": "NODE_ENV=production node ./dist/src/server.js",
    "prod": "npm-run-all db:migrate:apply db:migrate:status serve:prod",
    "lint": "tsc --noEmit && eslint ./src ./test --ext=js,ts --fix",
    "test": "NODE_ENV=test node node_modules/mocha/bin/_mocha -r ts-node/register test/data-source.api.test.ts --exit --timeout 10000"
  },
  "type": "commonjs",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.6.0",
    "app-root-path": "^3.0.0",
    "asyncapi-validator": "^2.4.5",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validate": "^0.5.1",
    "js-yaml": "^3.14.0",
    "keycloak-connect": "^11.0.0",
    "lodash": "^4.17.20",
    "migrate-mongo": "^8.1.1",
    "mongoose": "^5.9.29",
    "npm-run-all": "^4.1.5",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/app-root-path": "^1.2.4",
    "@types/chai": "^4.2.12",
    "@types/config": "0.0.36",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/js-yaml": "^3.12.5",
    "@types/keycloak-connect": "^4.5.4",
    "@types/lodash": "^4.14.159",
    "@types/mocha": "^8.0.2",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.0.27",
    "@types/sinon": "^9.0.4",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "chai": "^4.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^4.1.0",
    "mocha": "^8.1.1",
    "mongodb-memory-server": "^6.6.3",
    "prettier": "^2.0.5",
    "sinon": "^9.0.3",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.57",
    "typescript": "^3.9.7"
  }
}
